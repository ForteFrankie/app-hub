        "inert": "^5.1.2",
        "joi": "^14.3.1",
        "jwks-rsa": "^1.3.0",
        "knex": "^0.16.3",
        "pg": "^7.8.0",
        "uuid": "^3.3.2",
        "vision": "^5.4.4"
    },
    "devDependencies": {
        "code": "^5.2.4",
        "lab": "^18.0.1",
        "nodemon": "^1.18.9",
        "sqlite3": "^4.0.6"
