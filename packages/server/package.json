    "private": true,
    "scripts": {
        "start": "NODE_ENV=development nodemon src/main.js",
        "test": "NODE_ENV=test npm run test-db && lab -a code -t 85 -v  -I 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__'",
